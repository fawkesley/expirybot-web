#!/bin/sh -eux

THIS_SCRIPT=$0
REPO_DIR=$(dirname ${THIS_SCRIPT})/..

find /tmp -user $USER -name '*list-keys.txt' -mtime +1 -delete
find /tmp -user $USER -name '*list-packets.txt' -mtime +1 -delete

. ${REPO_DIR}/script/_setup_environment

exec timeout 50m ${REPO_DIR}/manage.py delete_old_test_results
